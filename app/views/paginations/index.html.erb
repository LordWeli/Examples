<div class="box-body">
  <div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-6">
      <table class="table table-striped table-bordered table-sm">
        <thead>
          <tr>
            <th>Name
            </th>
          </tr>
        </thead>
        <tbody>
        <% @items.each do |item| %>
          <tr>
            <td> <%= item.name %> </td>
          </tr>
        <% end %>
          
        </tbody>
      </table>
      
      <%= paginate @items %>
    </div>
  </div>
</div>
